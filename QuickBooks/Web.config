<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
    <section name="databaseSettings" type="System.Configuration.NameValueSectionHandler, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
  </configSections>
  <common>
    <logging>
      <factoryAdapter type="Common.Logging.NLog.NLogLoggerFactoryAdapter, Common.Logging.NLog40">
        <arg key="configType" value="FILE" />
        <arg key="configFile" value="~/NLog.config" />
      </factoryAdapter>
    </logging>
  </common>
  <databaseSettings>
    <add key="db.datasource" value="localhost" />
    <add key="db.user" value="root" />
    <add key="db.password" value="alex" />
    <add key="db.database" value="QuickBooks" />
    <add key="db.connectionString" value="server=${db.datasource};user id=${db.user};password=${db.password};database=${db.database};persistsecurityinfo=False" />
  </databaseSettings>
  <spring>
    <context type="Spring.Context.Support.XmlApplicationContext, Spring.Core">
      <resource uri="config://spring/objects" />
    </context>
    <objects xmlns="http://www.springframework.net" xmlns:db="http://www.springframework.net/database" xmlns:tx="http://www.springframework.net/tx">
      <object id="HomeController" type="QuickBooks.Controllers.HomeController" autowire="autodetect" singleton="false" />
      <object id="ReportController" type="QuickBooks.Controllers.ReportController" autowire="autodetect" singleton="false" />
      <object id="CreditMemoService" type="QuickBooks.Models.EntityService.CreditMemoService" autowire="autodetect" singleton="true" />
      <object id="InvoiceService" type="QuickBooks.Models.EntityService.InvoiceService" autowire="autodetect" singleton="true" />
      <object id="SalesReceiptService" type="QuickBooks.Models.EntityService.SalesReceiptService" autowire="autodetect" singleton="true" />
      <object id="OAuthService" type="QuickBooks.Models.EntityService.OAuthService" autowire="autodetect" singleton="true" />
      <object id="OAuthRepository" type="QuickBooks.Models.Repository.OAuthRepository" autowire="autodetect" singleton="true" />
      <object id="ReportService" type="QuickBooks.Models.ReportService.ReportService" autowire="autodetect" singleton="true" />
      <object id="ReportRepository" type="QuickBooks.Models.Repository.ReportRepository" autowire="autodetect" singleton="true" />
      <object id="EstimateService" type="QuickBooks.Models.EntityService.EstimateService" autowire="autodetect" singleton="true" />
      <object id="BytecodeProvider" type="Spring.Data.NHibernate.Bytecode.BytecodeProvider" autowire="autodetect" singleton="true"/>
      <object id="EncryptedString" type="QuickBooks.Models.DAL.EncryptedString" autowire="autodetect" singleton="true"/>
      <object id="CryptoEngine" type="QuickBooks.Models.EntityService.CryptoEngine" autowire="autodetect" singleton="true">
        <constructor-arg name="securityKey" value="tyurptz" />
      </object>
      <object type="Spring.Objects.Factory.Config.PropertyPlaceholderConfigurer, Spring.Core">
        <property name="ConfigSections" value="databaseSettings" />
      </object>
      <db:provider id="DbProvider" provider="MySql" connectionString="${db.connectionString}" />
      <object id="NHibernateSessionFactory" type="Spring.Data.NHibernate.LocalSessionFactoryObject, Spring.Data.NHibernate4">
        <property name="DbProvider" ref="DbProvider" />
        <property name="BytecodeProvider" ref="BytecodeProvider" />
        <property name="MappingAssemblies">
          <list>
            <value>QuickBooks</value>
          </list>
        </property>
        <property name="HibernateProperties">
          <dictionary>
            <entry key="connection.provider" value="NHibernate.Connection.DriverConnectionProvider" />
            <entry key="connection.driver_class" value="NHibernate.Driver.MySqlDataDriver" />
            <entry key="dialect" value="NHibernate.Dialect.MySQL5Dialect" />
<!--            <entry key="hbm2ddl.auto" value="create" />-->
          </dictionary>
        </property>
        <property name="ExposeTransactionAwareSessionFactory" value="true" />
      </object>
      <object id="transactionManager" type="Spring.Data.NHibernate.HibernateTransactionManager, Spring.Data.NHibernate4">
        <property name="DbProvider" ref="DbProvider" />
        <property name="SessionFactory" ref="NHibernateSessionFactory" />
        <property name="NestedTransactionsAllowed" value="true" />
      </object>

    </objects>
  </spring>
  <appSettings>
    <add key="nhibernate-logger" value="NHibernate.Logging.CommonLogging.CommonLoggingLoggerFactory, NHibernate.Logging.CommonLogging" />
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--OAuth -->
    <add key="ConsumerKey" value="qyprdDzmuM0rBxHAxboVdt6hEmay8w" />
    <add key="ConsumerSecret" value="OzAkWoEON3suNDtAwujZEO2M6YjqexTpdBjMELmP" />
    <add key="appToken" value="926acffab65abb4251b9d29b3d7daf2f4bf5" />
    <!-- Configurations -->
    <add key="OauthLink" value="https://oauth.intuit.com/oauth/v1" />
    <add key="ServiceContext.BaseUrl.Qbo" value="https://sandbox-quickbooks.api.intuit.com/" />
    <add key="AuthorizeUrl" value="https://appcenter.intuit.com/Connect/Begin" />
    <add key="GET_REQUEST_TOKEN" value="https://oauth.intuit.com/oauth/v1/get_request_token" />
    <add key="GET_ACCESS_TOKEN" value="https://oauth.intuit.com/oauth/v1//get_access_token" />
    <add key="DeepLink" value="sandbox.qbo.intuit.com" />
  </appSettings>
